{
  "name": "freebusy",
  "version": "0.0.0",
  "description": "Determine free blocks from a list of events and free/busy rules.",
  "license": "ISC",
  "repository": "metaraine/freebusy",
  "author": {
    "name": "Raine Lourie",
    "url": "https://github.com/metaraine"
  },
  "engines": {
    "node": "0.12.x"
  },
  "config": {
    "sweet": {
      "in": "src/*.js",
      "out": "lib/index.js"
    }
  },
  "main": "$npm_package_config_sweet_out",
  "scripts": {
    "test": "mocha",
    "watch": "nodemon -w src -w test -x 'npm run build && npm run test'",
    "build": "mkdir -p lib && sjs -m lambda-chop/macros -o $npm_package_config_sweet_out $npm_package_config_sweet_in"
  },
  "keywords": [
    "calendar",
    "schedule",
    "free",
    "busy",
    "time",
    "conflict",
    "block"
  ],
  "dependencies": {
    "moment": "^2.10.6"
  },
  "devDependencies": {
    "chai": "^3.2.0",
    "lambda-chop": "^0.1.5",
    "mocha": "^2.2.5",
    "nodemon": "^1.4.0",
    "sweet.js": "^0.7.4"
  }
}
